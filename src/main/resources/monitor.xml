<monitor>
    <name>AnalyticsMetrics</name>
    <type>managed</type>
    <description>Execute analytics metrics and report as custom metrics</description>
    <enabled>true</enabled>
    <monitor-configuration>
    </monitor-configuration>
    <monitor-run-task>
        <execution-style>periodic</execution-style> 
        <execution-frequency-in-seconds>60</execution-frequency-in-seconds> 
        <execution-timeout-in-secs>60</execution-timeout-in-secs>
        <name>Run</name>
        <type>executable</type>
        <task-arguments>
        </task-arguments>
        <executable-task>
            <type>file</type>
            <file>run.bat</file>
        </executable-task>
    </monitor-run-task>
</monitor>
